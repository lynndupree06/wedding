<div class="tab-pane active" id="engagement">
  <h1>Engagement Photos</h1>
  <div class="photos">
    <% idx = 0 %>
    <% @photos[:engagement].each do |p| %>
        <%= image_tag p.avatar.url(:medium), :class => 'image', :id => "engagement-#{idx}" %>
        <% idx = idx + 1 %>
    <% end %>
  </div>
</div>